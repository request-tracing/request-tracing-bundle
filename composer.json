{
    "name": "request-tracing/request-tracing-bundle",
    "description": "Symfony bundle for request tracing",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "othillo",
            "email": "othillo@othillo.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4||^6.3",
        "symfony/yaml": "^5.4||^6.3",
        "monolog/monolog": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "phpstan/phpstan": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.8",
        "psr/http-message": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "thecodingmachine/phpstan-strict-rules": "^1.0",
        "symfony/browser-kit": "^4.4||^5.4||^6.0",
        "symfony/monolog-bundle": "^3.7",
        "guzzlehttp/guzzle": "^7.0",
        "matthiasnoback/symfony-config-test": "^4.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpstan/phpstan-symfony": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "RequestTracing\\RequestTracingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RequestTracing\\RequestTracingBundle\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
